/* pages/newUserRegister/newUserRegister.wxss */
.new-userBTN{
  width:300rpx;
  height:96rpx;
  background:rgba(187,32,42,1);
  border-radius:48rpx;
  text-align: center;
  line-height: 96rpx;
  color: #fff;
  font-size: 32rpx;
  margin: 0 auto;
  margin-top: 30rpx;
}
.top-img{
  height: 1000rpx;
  position: relative;
}
.top-img image{
  width: 100%;
  height: 1000rpx;
}
.rule{
  position: absolute;
  top: 20rpx;
  right: 20rpx;
  font-size: 32rpx;
  color: #fff;
  width: 100rpx;
  height: 40rpx;
  border: 1px solid #eee;
  text-align: center;
  line-height: 40rpx;
}